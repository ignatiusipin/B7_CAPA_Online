<!DOCTYPE html>
<html lang="en">

<head>
    <meta charset="utf-8">
    <meta http-equiv="X-UA-Compatible" content="IE=edge">
    <meta name="viewport" content="width=device-width,initial-scale=1">
    <title>CAPA ONLINE</title>
    <!-- Favicon icon -->

    <link rel="icon" type="image/png" sizes="16x16" href="~/Content/Styles/images/favicon.png">
    <!-- Custom Stylesheet -->
    <link href="~/Content/Styles/css/style.css" rel="stylesheet" />
    <link href="~/Content/Styles/css/capa-style.css" rel="stylesheet" />
    <link href="~/Content/Styles/css/vendor-style.css" rel="stylesheet" />

    <!-- CSS Date Picker -->
    <!-- Custom Stylesheet -->
    <link href="~/Content/Scripts/plugins/bootstrap-material-datetimepicker/css/bootstrap-material-datetimepicker.css" rel="stylesheet">
    <!-- Page plugins css -->
    <link href="~/Content/Scripts/plugins/clockpicker/dist/jquery-clockpicker.min.css" rel="stylesheet">
    <!-- Color picker plugins css -->
    <link href="~/Content/Scripts/plugins/jquery-asColorPicker-master/css/asColorPicker.css" rel="stylesheet">
    <!-- Date picker plugins css -->
    <link href="~/Content/Scripts/plugins/bootstrap-datepicker/bootstrap-datepicker.min.css" rel="stylesheet">
    <!-- Daterange picker plugins css -->
    <link href="~/Content/Scripts/plugins/timepicker/bootstrap-timepicker.min.css" rel="stylesheet">
    <link href="~/Content/Scripts/plugins/bootstrap-daterangepicker/daterangepicker.css" rel="stylesheet">
    <!-- CSS Date Picker END -->
    <link href="~/Content/Scripts/plugins/sweetalert/css/sweetalert.css" rel="stylesheet" />
    <link href="~/Content/Styles/css/select2.min.css" rel="stylesheet" />
    @*datatables*@
    <link href="~/Content/Styles/css/datatables.min.css" rel="stylesheet" />
    <!-- JS -->

    <script src="https://cdnjs.cloudflare.com/ajax/libs/moment.js/2.29.1/moment.min.js" integrity="sha512-qTXRIMyZIFb8iQcfjXWCO8+M5Tbc38Qi5WzdPOYZHIlZpzBHG3L3by84BBBOiRGiEb7KKtAOAs5qYdUiZiQNNQ==" crossorigin="anonymous" referrerpolicy="no-referrer"></script>
    <script src="~/Content/Scripts/plugins/datatables/jquery.dataTables.min.js" defer></script>
    <script src="~/Content/Scripts/plugins/jquery/jquery.min.js"></script>

    <link href="~/Content/Scripts/plugins/toastr/css/toastr.min.css" rel="stylesheet" />

    <link href="~/Content/Scripts/plugins/auto-complete/jquery-ui.min.css" rel="stylesheet" />
</head>

<body>
    <!--*******************
        Preloader start
    ********************-->
    <div id="preloader">
        <div class="loader">
            <svg class="circular" viewBox="25 25 50 50">
                <circle class="path" cx="50" cy="50" r="20" fill="none" stroke-width="3" stroke-miterlimit="10" />
            </svg>
        </div>
    </div>
    <!--*******************
        Preloader end
    ********************-->
    <!--**********************************
        Main wrapper start
    ***********************************-->
    <div id="main-wrapper">

        <!--**********************************
            Nav header start
        ***********************************-->
        <div class="nav-header">
            <div class="brand-logo">
                <a href="../HomePage/Index">
                    <b class="logo-abbr"><img src="~/Content/Styles/images/logo.png" alt=""> </b>
                    <span class="logo-compact"><img src="~/Content/Styles/images/logo-compact.png" alt=""></span>
                    <span class="brand-title">
                        <img src="~/Content/Styles/images/logo-text.png" alt="">
                    </span>
                </a>
            </div>
        </div>
        <!--**********************************
            Nav header end
        ***********************************-->
        <!--**********************************
            Header start
        ***********************************-->
        <div class="header">
            <div class="header-content clearfix">
                <div class="nav-control">
                    <div class="hamburger">
                        <span class="toggle-icon"><i class="icon-menu"></i></span>
                    </div>
                </div>

                <div class="header-left">
                    <div class="input-group icons">
                        <div class="input-group-prepend">
                            <h1 style="color:white"><marquee width="846">CAPA Online</marquee></h1>
                        </div>

                    </div>

                </div>


                <div class="header-right">
                    <ul class="clearfix">
                        <li class="icons dropdown d-none d-md-flex">
                            <a href="javascript:void(0)" class="log-user" data-toggle="dropdown">
                                Selamat datang, <h5 id="UserLoginXd" style="color:white"></h5>
                            </a>
                        </li>
                        <li class="icons dropdown">
                            <div class="user-img c-pointer position-relative" data-toggle="dropdown">
                                <span class="activity active"></span>
                                <img src="~/Content/Styles/images/user/1.png" height="40" width="40" alt="">
                            </div>
                            <div class="drop-down dropdown-profile   dropdown-menu">
                                <div class="dropdown-content-body my-2">
                                    <ul>
                                        <li>
                                            <a id="spanLogout" href="@Url.Action("Index", "Login")"><i class="icon-key"></i> <span>Logout</span></a>
                                        </li>
                                    </ul>
                                </div>
                            </div>
                        </li>
                    </ul>
                </div>
            </div>
        </div>
        <!--**********************************
            Header end ti-comment-alt
        ***********************************-->
        <!--**********************************
            Sidebar start
        ***********************************-->
        <div class="nk-sidebar">
            <div class="nk-nav-scroll">
                <ul class="metismenu" id="menu">
                    <li class="nav-label">Dashboard</li>
                    <li>@Html.ActionLink("Create CAPA", "CreateCAPA", "Koordinator", null, null)</li>
                    <li>@Html.ActionLink("Pending Task", "TaskList", "PendingTask", null, null)</li>
                    <li>@Html.ActionLink("Pelaksana", "PelaksanaCAPA", "Pelaksana", null, null)</li>
                    <li class="nav-label">VENDOR</li>
                    <li>
                        <a class="has-arrow" href="javascript:void()" aria-expanded="true">
                            <i class="icon-list menu-icon"></i><span class="nav-text">List Vendor</span>
                        </a>
                        <ul aria-expanded="true">
                            <li>@Html.ActionLink("List Vendor", "Index", "Vendor", null, null)</li>
                            <li>@Html.ActionLink("Create Vendor", "CreateVendor", "Vendor", null, null)</li>
                        </ul>
                    </li>
                    <li>
                        <a class="has-arrow" href="javascript:void()" aria-expanded="true">
                            <i class="icon-user menu-icon"></i><span class="nav-text">List User Vendor</span>
                        </a>
                        <ul aria-expanded="true">
                            <li>@Html.ActionLink("List User Vendor", "Index", "UserVendor", null, null)</li>
                            <li>@Html.ActionLink("Create User Vendor", "CreateUserVendor", "UserVendor", null, null)</li>
                        </ul>
                    </li>
                </ul>
            </div>
        </div>
        <!--**********************************
            Sidebar end
        ***********************************-->
        <!--**********************************
            Content body start
        ***********************************-->
        @RenderBody()
        <!--**********************************
            Content body end
        ***********************************-->
        <!--**********************************
            Footer start
        ***********************************-->
        <div style="background-color:transparent">
            <div class="container-fluid">
                <div class="has-arrow">
                    <div class="col-lg-12">
                        <p style="text-align:center">Copyright &copy; @DateTime.Now.Year PT Bintang Toedjoe </p>
                    </div>
                </div>
            </div>
        </div>

        <div class="modal fade" id="ModalTimeOut">
            <div class="modal-dialog modal-dialog-centered" role="document">
                <div class="modal-content">
                    <div class="modal-header">
                        <h5 class="modal-title">Session Login Habis</h5>
                        <button type="button" class="close" data-dismiss="modal">
                            <span>&times;</span>
                        </button>
                    </div>
                    <div class="modal-body">
                        <p id="msgModal"></p>
                    </div>

                </div>
            </div>
        </div>
        <!--**********************************
            Footer end
        ***********************************-->
    </div>
    <!--**********************************
        Main wrapper end
    ***********************************-->
    <!--**********************************
        Scripts
    ***********************************-->
    <script src="~/Content/Scripts/plugins/common/common.min.js"></script>
    <script src="~/Content/Scripts/js/custom.min.js"></script>
    <script src="~/Content/Scripts/js/settings.js"></script>
    <script src="~/Content/Scripts/js/gleek.js"></script>
    <script src="~/Content/Scripts/js/styleSwitcher.js"></script>

    <script src="https://cdnjs.cloudflare.com/ajax/libs/sweetalert/2.1.2/sweetalert.min.js"></script>
    @*<script src="~/Content/Scripts/plugins/sweetalert/js/sweetalert.init.js"></script>*@
    @*<script src="~/Content/Scripts/js/sweetalert2.all.min.js"></script>*@

    <!-- Date Picker Plugin JavaScript -->
    <script src="~/Content/Scripts/plugins/moment/moment.js"></script>
    <script src="~/Content/Scripts/plugins/bootstrap-material-datetimepicker/js/bootstrap-material-datetimepicker.js"></script>


    <!-- Clock Plugin JavaScript -->
    <script src="~/Content/Scripts/plugins/clockpicker/dist/jquery-clockpicker.min.js"></script>


    <!-- Color Picker Plugin JavaScript -->
    <script src="~/Content/Scripts/plugins/jquery-asColorPicker-master/libs/jquery-asColor.js"></script>
    <script src="~/Content/Scripts/plugins/jquery-asColorPicker-master/libs/jquery-asGradient.js"></script>
    <script src="~/Content/Scripts/plugins/jquery-asColorPicker-master/dist/jquery-asColorPicker.min.js"></script>


    <!-- Date Picker Plugin JavaScript -->
    <script src="~/Content/Scripts/plugins/bootstrap-datepicker/bootstrap-datepicker.min.js"></script>


    <!-- Date range Plugin JavaScript -->
    <script src="~/Content/Scripts/plugins/timepicker/bootstrap-timepicker.min.js"></script>
    <script src="~/Content/Scripts/plugins/bootstrap-daterangepicker/daterangepicker.js"></script>

    <script src="~/Content/Scripts/js/plugins-init/form-pickers-init.js"></script>

    <script src="~/Content/Scripts/plugins/validation/jquery.validate.min.js"></script>
    <script src="~/Content/Scripts/plugins/validation/jquery.validate-init.js"></script>

    <script src="~/Content/Scripts/plugins/select2/js/select2.full.min.js"></script>
    <script src="~/Content/Scripts/plugins/toastr/js/toastr.min.js"></script>
    <script src="~/Content/Scripts/plugins/auto-complete/jquery-ui.min.js"></script>

    @*datatables*@
    <script src="~/Content/Scripts/js/datatables.min.js"></script>

    <script src="~/Content/Scripts/plugins/highlightjs/highlight.pack.min.js"></script>
    <script>hljs.initHighlightingOnLoad();</script>

    <script>
        $(document).ready(function () {
            var usernameAktif = '@Request.RequestContext.HttpContext.Session["Username"]';
            $('#UserLoginXd').text(usernameAktif);
        });
        (function ($) {
            "use strict"

            new quixSettings({
                version: "light", //2 options "light" and "dark"
                layout: "vertical", //2 options, "vertical" and "horizontal"
                navheaderBg: "color_1", //have 10 options, "color_1" to "color_10"
                headerBg: "color_8", //have 10 options, "color_1" to "color_10"
                sidebarStyle: "vertical", //defines how sidebar should look like, options are: "full", "compact", "mini" and "overlay". If layout is "horizontal", sidebarStyle won't take "overlay" argument anymore, this will turn into "full" automatically!
                sidebarBg: "color_1", //have 10 options, "color_1" to "color_10"
                sidebarPosition: "static", //have two options, "static" and "fixed"
                headerPosition: "fixed", //have two options, "static" and "fixed"
                containerLayout: "wide",  //"boxed" and  "wide". If layout "vertical" and containerLayout "boxed", sidebarStyle will automatically turn into "overlay".
                direction: "ltr" //"ltr" = Left to Right; "rtl" = Right to Left
            });


        })(jQuery);
    </script>

</body>

</html>