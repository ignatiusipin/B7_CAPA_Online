
@{
    ViewBag.Title = "Session_Error";
    Layout = "~/Views/Shared/_Layout.cshtml";
}


<script>
         $(document).ready(function () {
            @*CheckUser();*@
            var usernameAktif = '@Request.RequestContext.HttpContext.Session["Username"]';
             $('#UserLoginXd').text(usernameAktif);
             holdchcksession();
            
         });
        async function holdchcksession() {
            await chcksession();
        }
        function chcksession() {
            Swal.fire({
                title: 'Session Expired! Silahkan login Kembali ',
                icon: "warning",
                showConfirmButton: false,
                timer: 1000,
            }).then(() => {
                return window.location.href = '../Login/Index';
            });
        }
</script>
  