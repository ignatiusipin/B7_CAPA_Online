                        $('#btn_AddKajian').attr("disabled", false);
                        $('#RootCause_KajianResikoDDL').append(new Option(dataPerbaikan.Tindakan_Perbaikan, dataPerbaikan.Tindakan_Perbaikan));
                        $('#RootCause_KajianResikoDDL').val(dataPerbaikan.Tindakan_Perbaikan);

                    }
                    else {
                        $('#btn_AddKajian').attr("disabled", true);
                    }
                    if (dataPerbaikan.Status_ID != 6) {
                        $('#btn_AddKajian').attr("disabled", true);
                        $('#txtTindakanPerbaikanRev').attr("disabled", true);
                        $('#AlasanTindakanPerbaikan').attr("disabled", true);
                        $('#dueDatePerbaikanRev').attr("disabled", true);
                        $('#AlasanDueDate').attr("disabled", true);
                        $('#UpdatePerbaikan').attr("disabled", true);
                        $('#Submit').attr("disabled", true);

                    }
                    @* $('#txtAreaBerpotensi').val(dataPerbaikan.WhyDesc);*@

                },
